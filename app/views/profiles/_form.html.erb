<div class="col-9">
  <%= form_for profile do |f| %>
    <div class="form-group">
      <%= f.label :full_name %>
      <%= f.text_field :full_name, class: 'form-control' %>
    </div>
    
    <div class="form-group">
      <%= f.label :social_name %>
      <%= f.text_field :social_name, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :birthdate %>
      <%= f.date_field :birthdate, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :qualifications %>
      <%= f.text_field :qualifications, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_area :description, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :experience %>
      <%= f.text_area :experience, class: 'form-control' %>
    </div>
  
    <div class="form-group">
      <%= f.label :picture %>
      <%= f.file_field :picture, accept: 'image/png,image/jpeg', class: 'form-control-file' %>
    </div>
  
    <%= f.submit t('.save'), class: 'btn btn-blue' %>
  <% end %>
</div>